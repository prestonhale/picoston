pico-8 cartridge // http://www.pico-8.com
version 39
__lua__
#include tyler.p8
#include michael.p8
#include preston.p8

objects = {}

speed = 1

debug = "no debug"


SPAWN_FUNCTIONS = {
    add_ostrich_in_lane,
    add_bee_giant_in_lane,
    add_bee_giant_in_lane,
    add_bee_giant_in_lane,
    add_bee_giant_in_lane
}

function _init()
    init_michael(objects)
    init_preston(objects)
    init_tyler(objects)
end

function _update()
    for obj in all(objects) do
        obj.type.update(obj)
    end
end

function _draw()
    for obj in all(objects) do
        obj.type.draw(obj)
    end
    print(debug)
end

-- helper functions
function get_lane_y(lane_index)
    return (lane_index-1)*lane_length
end
