pico-8 cartridge // http://www.pico-8.com
version 38
__lua__
#include tyler.p8
#include michael.p8
#include preston.p8

objects = {}

speed = 1

animal = {
    x = 0,
    y = 0,
    lane_x = 0,
    lane_y = 0,
    draw = function()
    end,
    update = function()
    end
}

function _init()
  --  init_preston(objects)
	init_tyler(objects)
    init_michael(objects)
end

function _update()
    for obj in all(objects) do
        obj.type.update(obj)
    end
	if btnp(5) then 
		
		add_ostrich_at(0,66) 
		
		end
end

function _draw()
				draw_background()
    for obj in all(objects) do
        obj.type.draw(obj)
    end
end

-- helper functions

lane_spacing=21
line_spacing=2
line_length=6
function draw_background()
	cls(3)
    map()
    for i=0,4 do
        curr_lane_x=0
        while (curr_lane_x < 130) do
            rect(curr_lane_x,i*lane_spacing+lane_spacing,curr_lane_x+line_spacing,i*lane_spacing+lane_spacing,7)
            curr_lane_x+=line_length
        end
    end
end
